{
  "name": "great.gov.uk",
  "version": "1.1.0",
  "engines": {
    "node": "5.6.0"
  },
  "private": true,
  "dependencies": {
    "express": "^4.13.4"
  },
  "scripts": {
    "start": "npm run build && node lib/server.js",
    "dev": "./lib/init-dev.sh && nodemon -e json,js,sh,html,yaml,md,css,scss lib/server.js  --ignore build",
    "postinstall": "npm run build",
    "build": "DEBUG=build,WARN,ERROR,$DEBUG node lib/build.js",
    "deploy": "npm run build && node_modules/iigb-aws-helpers/deploy.sh && node_modules/iigb-aws-helpers/invalidate-cache.sh"
  },
  "devDependencies": {
    "debug": "^2.2.0",
    "iigb-beta-content": "uktrade/iigb-beta-content",
    "great.gov.uk-structure": "uktrade/great.gov.uk-structure",
    "iigb-aws-helpers": "uktrade/iigb-aws-helpers",
    "js-yaml": "^3.6.1",
    "metalsmith": "^2.2.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-json": "^0.1.2",
    "metalsmith-layouts": "^1.6.5",
    "metalsmith-markdown": "^0.2.1",
    "metalsmith-sass": "^1.3.0",
    "nodemon": "^1.11.0",
    "nunjucks": "^2.5.2",
    "nunjucks-date": "^1.2.0",
    "underscore": "^1.8.3"
  }
}
